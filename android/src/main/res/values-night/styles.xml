<?xml version="1.0" encoding="utf-8"?>
<resources>

    <style name="AppTheme.Widget.AppBarLayout" parent="Widget.MaterialComponents.AppBarLayout.Surface">
        <item name="colorPrimary">?colorPrimary</item>
        <item name="android:elevation">@dimen/app_bar_layout_elevation</item>
        <item name="android:colorBackground">@color/grey_B_15</item>
        <item name="android:colorPrimaryDark">@color/grey_B_15</item>
    </style>


    <style name="AppTheme.Widget.BottomNavigationView" parent="Widget.MaterialComponents.BottomNavigationView">
        <item name="android:colorBackground">@color/grey_B_15</item>
        <item name="android:panelColorBackground">@color/grey_B_15</item>
    </style>


    <style name="AppTheme.Widget.Spinner" parent="Widget.AppCompat.Spinner">
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>

    <style name="AppTheme.Widget.SpinnerItem" parent="Widget.AppCompat.TextView.SpinnerItem">
        <item name="android:textColor">?attr/colorOnPrimary</item>
    </style>


</resources>